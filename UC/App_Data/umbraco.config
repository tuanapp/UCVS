</LandingPage><LandingPage id="1354" key="e8a1bad9-ee02-4460-9804-476245251149" parentID="1187" level="2" creatorID="0" sortOrder="9" createDate="2017-06-19T03:21:17" updateDate="2017-06-20T02:05:04" nodeName="About" urlName="about" path="-1,1187,1354" isDoc="" nodeType="1071" creatorName="UC Admin" writerName="UC Admin" writerID="0" template="1067" nodeTypeAlias="LandingPage">
  <umbracoNaviHide>0</umbracoNaviHide>
  <content><![CDATA[{
  "name": "1 column layout",
  "sections": [
    {
      "grid": 12,
      "rows": [
        {
          "name": "Image w. text",
          "areas": [
            {
